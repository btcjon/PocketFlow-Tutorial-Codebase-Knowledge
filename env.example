# GitHub token for repository access
GITHUB_TOKEN=

# Default LLM provider to use (openrouter, gemini, anthropic, openai)
LLM_PROVIDER=gemini

# OpenRouter settings
OPENROUTER_API_KEY=
OPENROUTER_MODEL=anthropic/claude-3-opus:beta

# Google Gemini settings
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-pro

# Anthropic Claude settings
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-opus-20240229

# OpenAI settings
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# Enable/disable caching (true/false)
USE_CACHE=true 